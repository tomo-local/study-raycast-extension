{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-notes",
  "title": "Quick Notes",
  "description": "簡単にメモを取れるRaycast拡張機能",
  "icon": "note-icon.png",
  "author": "your-name",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "create-note",
      "title": "create note",
      "description": "新しいメモを作成します",
      "mode": "view",
      "arguments": [
        {
          "name": "title",
          "type": "text",
          "placeholder": "ファイル名を入力"
        }
      ]
    },
    {
      "name": "list-notes",
      "title": "list notes",
      "description": "保存されたメモの一覧を表示します",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
